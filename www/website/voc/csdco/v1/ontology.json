}, {
  "@id" : "http://opencoredata.org/voc/csdco/v1/SRF",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "http://schema.org/Dataset", "http://www.w3.org/2002/07/owl#namedIndividual" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Locations and information about samples that have been pulled from the cores."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "dcat:Dataset"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
