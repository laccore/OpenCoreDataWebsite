  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : ""
  } ]
}, {
  "@id" : "http://opencoredata.org/voc/csdco/v1/Grainsize",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "http://schema.org/Dataset", "http://www.w3.org/2002/07/owl#namedIndividual" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Measures the particle size distribution of sediment samples using laser diffraction analysis"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "dcat:Dataset"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Grain Size"
  } ]
}, {
  "@id" : "http://opencoredata.org/voc/csdco/v1/ICDFiles",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "http://schema.org/Dataset", "http://www.w3.org/2002/07/owl#namedIndividual" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "PSICAT is a software application for lithologic descriptions of cores using a set of controlled vocabularies and patterns for lithologies. The PSICAT folders contain reduced core images, .xml files with core description notes."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "dcat:Dataset"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Lithologic core description"
  } ]
}, {
  "@id" : "http://opencoredata.org/voc/csdco/v1/Image",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "http://schema.org/Dataset", "http://www.w3.org/2002/07/owl#namedIndividual" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "The Geotek MSCL-CIS (Core Imaging System) collects ultra high resolution images of core samples with a digital linescan camera. The light and camera lens have polarizing filters to reduce glare on the core surface in the image. Images resolution is specified by the user (25 to 100 micron pixel size, typically 50 micron)."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "dcat:Dataset"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Geotek CIS (jpg)"
  } ]
}, {
  "@id" : "http://opencoredata.org/voc/csdco/v1/LOI",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "http://schema.org/Dataset", "http://www.w3.org/2002/07/owl#namedIndividual" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Water, organic matter, carbonate mineral, and siliciclastic+diatom content are estimated by sequentially measuring weight loss in sediment core subsamples after heating at selected temperatures."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@value" : "dcat:Dataset"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Loss-on Ignition"
  } ]
